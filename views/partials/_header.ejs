
<% if (username) { %>
  Welcome <%= username %>
  <form action="/logout" method="POST">
      <button>Logout</button>
  </form
<% } else { %>
  <p>Log in!</p>
  <form action="/login" method="POST">
      <input id="username" type="text" name="username">
      <input type="submit" value="Submit">
  </form>
<% } %>

